{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "3.1.0",
  "packageManager": "pnpm@10.7.1",
  "description": "Jiakun's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm build && config-inspector --basePath=./playground",
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync @jiakun-zhao/eslint-config",
    "stub": "unbuild --stub",
    "test": "vitest",
    "prebuild": "pnpm script:typegen && pnpm script:stylistic",
    "script:typegen": "tsx ./scripts/typegen.ts",
    "script:stylistic": "tsx ./src/plugins/stylistic/script.ts"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/utils": "^9.2.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/config-inspector": "^1.0.2",
    "@jiakun-zhao/utils": "^1.3.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.14.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-typegen": "^2.1.0",
    "knitwork": "^1.2.0",
    "local-pkg": "^1.1.1",
    "scule": "^1.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.1"
  }
}