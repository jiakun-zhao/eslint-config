{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "3.1.0",
  "packageManager": "pnpm@9.11.0",
  "description": "Jiakun's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "inspector": "config-inspector",
    "inspector-antfu": "config-inspector --basePath=./antfu",
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync @jiakun-zhao/eslint-config",
    "stub": "unbuild --stub",
    "dev": "tsx src/index.ts"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@stylistic/eslint-plugin": "^2.8.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^0.2.5",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-perfectionist": "^3.6.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "typescript-eslint": "^8.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@eslint/config-inspector": "^0.5.4",
    "@types/node": "^22.5.5",
    "bumpp": "^9.5.2",
    "eslint": "^9.11.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  }
}
