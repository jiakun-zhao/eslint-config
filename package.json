{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@8.15.4",
  "description": "Jiakun's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "main": "dist/index.mjs",
  "files": [
    "dist/index.mjs",
    "package.json"
