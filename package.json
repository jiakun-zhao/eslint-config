{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "3.1.0",
  "packageManager": "pnpm@9.11.0",
  "description": "Jiakun's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync @jiakun-zhao/eslint-config",
    "stub": "unbuild --stub",
    "dev": "config-inspector --basePath=./playground"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.8.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-import-x": "^4.3.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/utils": "^0.7.10",
    "@eslint/config-inspector": "^0.5.4",
    "@types/node": "^22.7.4",
    "bumpp": "^9.6.1",
    "eslint": "^9.11.1",
    "eslint-typegen": "^0.3.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  }
}