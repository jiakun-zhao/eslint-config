{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "3.1.0",
  "packageManager": "pnpm@9.10.0",
  "description": "Jiakun's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync @jiakun-zhao/eslint-config",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.0",
    "@types/node": "^22.5.4",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  }
}
