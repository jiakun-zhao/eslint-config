{
    "name": "@unnb/eslint-config",
    "version": "1.0.1",
    "description": "Wythe's ESLint config",
    "author": "Wythe Zhao",
    "license": "ISC",
    "homepage": "https://github.com/unnb/eslint-config",
    "keywords": [
        "eslint-config"
    ],
    "scripts": {
        "env:set": "proxy && nrm use npm",
        "env:unset": "nrm use taobao && unproxy",
        "release": "pnpm env:set && bumpp --commit --tag --push && pnpm publish --access public && pnpm env:unset"
    },
    "main": "index.cjs",
    "files": [
        "index.cjs",
        "package.json"
    ],
    "peerDependencies": {
        "eslint": ">=7.4.0"
    },
    "dependencies": {
        "@antfu/eslint-config": "^0.34.0"
    },
    "devDependencies": {
        "bumpp": "^8.2.1",
        "eslint": "^8.31.0"
    }
}
