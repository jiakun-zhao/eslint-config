{
  "name": "@jiakun-zhao/eslint-config",
  "type": "module",
  "version": "4.1.8",
  "packageManager": "pnpm@10.27.0",
  "description": "Jiakun Zhao's ESLint config.",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "config-inspector --open=false",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync @jiakun-zhao/eslint-config",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typegen": "tsx ./scripts/typegen.ts"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-vue": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@unocss/eslint-plugin": "66.1.0-beta.12",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-oxlint": "^1.36.0",
    "eslint-plugin-perfectionist": "^5.2.0",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@eslint/core": "^1.0.0",
    "@jiakun-zhao/utils": "^1.4.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/types": "^8.51.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-typegen": "^2.3.0",
    "eslint-vitest-rule-tester": "^3.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.16"
  }
}
