    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unicorn": "^48.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.15.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.0",
    "@types/node": "^20.4.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
